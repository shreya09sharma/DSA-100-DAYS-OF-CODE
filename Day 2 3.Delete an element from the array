#include <stdio.h>

int main() {
    // Write C code here
    int n;
    printf("enter the size of an array\n");
    scanf("%d", &n);
    int arr[n];
    for(int i=0;i<n;i++)
{
    printf("elements %d=", i+1);
    scanf("%d", &arr[i]);
}
int index;
printf("enter the index\n");
scanf("%d", &index);
for(int i=index;i<n-1;i++)
{
    arr[i]=arr[i+1];
}
n--;
for(int i=0;i<n;i++)
{
    printf("%d", arr[i]);

}
    return 0;
}
