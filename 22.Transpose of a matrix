// Online C compiler to run C program online
#include <stdio.h>
int main() {
    int r,c;
    int j;
    int i;
printf("enter the number of rows\n");
scanf("%d", &r);
printf("enter the number of coloumns\n");
scanf("%d", &c);
int arr[r][c];
for(int i=0;i<r;i++)
{
    for(int j=0;j<c;j++)
    {
    printf("elements [%d][%d]=",i,j);
    scanf("%d",&arr[i][j]);
    }
}
printf("\n the elements of array are\n");
for(int a=0;a<r;a++)
{
    for(int p=0;p<c;p++)
    {
    printf("%d ", arr[a][p]);
}
printf("\n");
}
printf("\n the transpose of array is\n")
for(int a=0;a<c;a++)
{
    for(int p=0;p<r;p++)
    {
    printf("%d ", arr[p][a]);
}
printf("\n");
}
    return 0;
}
