#include <stdio.h>

int main() {
    int p, q, i, j, temp;
    scanf("%d", &p);
    int log1[p];
    for(i = 0; i < p; i++) 
    {
        scanf("%d", &log1[i]);
    }
    scanf("%d", &q);
    int log2[q];
    for(i = 0; i < q; i++)
    {
        scanf("%d", &log2[i]);
    }
    int merged[p + q];
    for(i = 0; i < p; i++) 
    {
        merged[i] = log1[i];
    }
    for(i = 0; i < q; i++) 
    {
        merged[p + i] = log2[i];
    }

    int total = p + q;
    for(i = 0; i < total - 1; i++) {
        for(j = 0; j < total - i - 1; j++) {
            if(merged[j] > merged[j + 1]) {
                temp = merged[j];
                merged[j] = merged[j + 1];
                merged[j + 1] = temp;
            }
        }
    }
    for(i = 0; i < total; i++)
    {
        printf("%d ", merged[i]);
    }

    return 0;
}
