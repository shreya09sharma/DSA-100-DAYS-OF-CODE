#include <stdio.h>

int main() {
    int n,temp,found;
    printf("enter the size of an array\n");
    scanf("%d", &n);
    int arr[n-1];
    for(int i=0;i<n-1;i++)
    {
        printf("elements %d =", i+1);
        scanf("%d", &arr[i]);
    }
  
    for(int i=0;i<n;i++)
    {
        found=0;
        for(int j=0;j<n-1;j++)
        {
            if(arr[j]==i+1)
            {
                found=1;
            }
        }
        if(found==0)
        {
            printf("the missing number is %d",i+1);
            break;
        }
        
    }


    return 0;
}
