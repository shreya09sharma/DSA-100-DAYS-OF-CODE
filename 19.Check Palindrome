// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int num;
    printf("enter a number\n");
    scanf("%d", &num);
int rev=0;
int temp=num;
    while(num!=0)
    {
        int d=num%10;
        rev=rev*10+d;
        num=num/10;
    }
    if(temp==rev)
    {
        printf("the number is palindrome \n");
    }
    else
    {
        printf("the number is not palindrome\n");
    }

    return 0;
}
