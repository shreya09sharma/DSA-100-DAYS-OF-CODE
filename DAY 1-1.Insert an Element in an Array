// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n;
    printf("enter the size of the array\n");
    scanf("%d", &n);
    int arr[n];
    for(int i=0;i<n;i++)
    {
        printf("elements %d=", i+1);
        scanf("%d", &arr[i]);
    }
int index;
printf("enter the index at which you want to store the element\n");
scanf("%d", &index);
int ele;
printf("enter the element\n");
scanf("%d", &ele);
for(int i=n;i>index;i--)
{
    arr[i]=arr[i-1];
}
    arr[index]=ele;
    n++;


printf("Array after insertion:\n");
for(int i = 0; i < n; i++)
{
printf("%d ", arr[i]);
}
    return 0;
}
