// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n;
    int a;
    printf("enter the size of an array\n");
    scanf("%d", &n);
    int nums[n];
    for(int i=0;i<n;i++)
    {
        printf("elements %d =", i+1);
        scanf("%d", &nums[i]);
    }
    int j=0;
    for(int i=0;i<n;i++)
    {

            if(nums[i]!=0)
            {
            
                nums[j]=nums[i];
                j++;
            }
    }
     while(j < n)
    {
        nums[j] = 0;
        j++;
    }
     printf("the element are:\n");
    for(int i = 0; i < n; i++)
    {
        printf("%d ", nums[i]);
    }

   

    return 0;
}
